void RaceScreenView::handleTickEvent() {
	carX = car.getX();
    carY = car.getY();
    //begin  direction finding
    if (carX < 240) {
    	carX++
    } else if (carX )
    carX++;Y--;
    //end
    carX
    car.moveRelative(x, y);
}
