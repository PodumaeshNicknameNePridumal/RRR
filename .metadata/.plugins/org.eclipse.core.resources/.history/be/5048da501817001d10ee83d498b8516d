#include <gui/model/Model.hpp>
#include <gui/model/ModelListener.hpp>


Model::Model() : modelListener(0)
{

}

void Model::tick()
{

#ifndef SIMULATOR
    static uint16_t msg = 0;
    if (osMessageQueueGet(joystickQueueHandle, &msg, 0, 0) == osOK)
    {
        modelListener->joystickMove(msg);
    }
#endif
}
